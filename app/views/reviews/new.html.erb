<div class="container">
  <h1>Add a new review</h1>
  <%= simple_form_for [@restaurant, @review] do |f| %>
    <%= f.input :content %>
    <%= f.input :rating %>
    <%= f.submit "Add a review", class: "btn btn-primary" %>
  <% end %>
  <%= link_to "Back to restaurant", restaurant_path(@restaurant) %>
</div>
